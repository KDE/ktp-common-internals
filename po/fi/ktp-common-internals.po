#, kde-format
msgctxt "Verbose user visible error string"
msgid "You have been disconnected"
msgstr "Yhteytesi on katkaistu"

#: KTp/error-dictionary.cpp:85
#, kde-format
msgctxt "Verbose user visible error string"
